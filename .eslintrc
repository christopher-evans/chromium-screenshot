{
  "extends": "airbnb-base",
  "rules": {
    "indent": [
      "error",
      4,
      {
        "SwitchCase": 1,
        "VariableDeclarator": 1,
        "outerIIFEBody": 1,
        "FunctionDeclaration": {
          "parameters": 1,
          "body": 1
        },
        "FunctionExpression": {
          "parameters": 1,
          "body": 1
        },
        "CallExpression": {
          "arguments": 1
        },
        "ArrayExpression": 1,
        "ObjectExpression": 1,
        "ImportDeclaration": 1,
        "flatTernaryExpressions": false
      }
    ],
    "camelcase": 0,
    "brace-style": [
      "error",
      "allman",
      {
        "allowSingleLine": false
      }
    ],
    "implicit-arrow-linebreak": 0,
    "prefer-const": [
      "warn",
      {
        "destructuring": "any",
        "ignoreReadBeforeAssign": true
      }
    ],
    "function-paren-newline": [
      "error",
      "consistent"
    ],
    "max-len": [
      "error",
      {
        "code": 120
      }
    ],
    "class-methods-use-this": 0,
    "comma-dangle": [
      "error",
      {
        "arrays": "never",
        "objects": "never",
        "imports": "never",
        "exports": "never",
        "functions": "never"
      }
    ],
    "quotes": [
      "error",
      "double"
    ],
    "operator-linebreak": [
      "error",
      "after"
    ],
    "prefer-template": 0,
    "no-bitwise": 0,
    "no-underscore-dangle": 0,
    "no-return-assign": 0,
    "quote-props": [
      "error",
      "always"
    ],
    "dot-notation": 0,
    "space-unary-ops": [
      1,
      {
        "words": true,
        "nonwords": true
      }
    ],
    "arrow-body-style": [
      "error",
      "as-needed"
    ],
    "arrow-parens": [
      "error",
      "as-needed"
    ],
    "no-param-reassign": [
      "error",
      {
        "props": false
      }
    ]
  }
}
